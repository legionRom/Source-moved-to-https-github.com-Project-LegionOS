<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- LegionOS remote -->
  <remote name="legion"
          fetch="https://github.com/Legionrom"
          revision="refs/heads/q"
          sync-j="8" />
  
  <!--LegionOS Devices -->
  <remote name="legionos-devices"
          fetch="https://github.com/LegionOS-Devices"
          revision="refs/heads/q"
          sync-j="8" />

<remote name="lineage" 
          fetch="https://github.com/LineageOS" 
          revision="refs/heads/lineage-17.1"
          sync-j="8" />

  <remote name="los" 
          fetch="https://github.com/LineageOS" 
          revision="refs/heads/lineage-17.1"
          sync-j="8" />

  <!--Legion Lab-->
  <remote name="leg-lab"
          fetch="https://gitlab.com/Rajkale99"
          revision="refs/heads/q"
          sync-j="8" />

  <remote name="github"
          fetch="https://github.com/" />
		   
  <!-- Removed projects -->	   
  <include name="remove.xml" />

  <!-- Imported projects -->
  <include name="legion/build.xml" />
  <include name="legion/device.xml" />
  <include name="legion/external.xml" />
  <include name="legion/frameworks.xml" />
  <include name="legion/hals.xml" />
  <include name="legion/packages.xml" />
  <include name="legion/prebuilts.xml" />
  <include name="legion/services.xml" />
  <include name="legion/system.xml" />
  <include name="legion/vendors.xml" />

</manifest>
